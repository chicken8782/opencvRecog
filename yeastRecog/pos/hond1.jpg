<script type="text/javascript">
	/* Delay show of website */
	function delayShow() {
			var secs = 1000;
			setTimeout('$("html").css("visibility","visible");', secs);
}
function WrapClass(x,y) {
	if (x.length) {
		x.each (function () {
			($(this).wrap(y));
		});
	}
}
function Moveblocktitle() {
		$("#footercontainer > .blocktitle").each(function(){
        var title = $(this).html();
        $(this).next().addClass('moveto');
        $('.moveto').prepend('<div class="footertitle">' +title+ '</div>');
        $(this).remove();
        $('.moveto').removeClass('moveto');
		});	
}
function mediaalbum() {
		$('.mediaalbumprevnext img').addClass('nextimg');
		($(".nextimg:first").attr("src","/style/oran20121108213902/images/left.png"));
		($(".nextimg:last").attr("src","/style/oran20121108213902/images/right.png"));
}
function mainmedia() {
		$(".mediaitemTN:nth-child(3n)").after('<div class="mediabreak"></div>');
		$("#block_main .mediaitemTN img").before('<span class="imagehover" style="opacity: 0;"></span>');
		$('#block_main .mediaitemTN').hover(
		function() {
			$(this).find('.imagehover').stop().fadeTo(500, 0.60);
		},
		function() {
			$(this).find('.imagehover').stop().fadeTo(500, 0);
		}
	)
}
function footer_display() {
	if ($.trim($('#block_footer').html()).length) {
	($('#footercontainer').children().addClass('footers'));
		 if ($('#footercontainer .blocktitle')) {
			($('.blocktitle').removeClass('footers'));
		} 
	($('#footercontainer .footers:first').addClass('first'));
	($('#footercontainer .footers:nth-child(4)').addClass('last'));
	($('#footercontainer .footers:gt(3)').remove()); 
	}
}	
	$(document).ready(function(){
		 WrapClass($('#block_main .media') , '<div class="mediacontainer" />');
		 Moveblocktitle();
		 footer_display();
		 if ($('#block_main .media').length) {
			mainmedia();
		 }
		 if ($('#mediaalbumajaxcontainer').length) {
		 mediaalbum();
		 }
		 delayShow();
		 });		 
</script>
